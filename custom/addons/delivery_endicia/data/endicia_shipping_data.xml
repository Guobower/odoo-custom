<odoo>
<data noupdate="0">
        <!--Resource: res.partner-->
        <record id="partner_usps" model="res.partner">
            <field name="name">Endicia</field>
            <field name="is_company" eval="True"/>
            <field name="supplier" eval="True"/>
            <field name="customer" eval="False"/>
        </record>

    <record id="delivery_carrier_endicia_usps_fc" model="delivery.carrier">
        <field name="name">USPS First-Class Mail</field>
        <field name="fixed_price">0</field>
        <field name="partner_id" ref="partner_usps"/>
        <field name="product_sale_ok" eval="False"/>
        <field name="product_type">service</field>
        <field name="delivery_type">endicia</field>
        <field name="endicia_service_type">First-Class Mail</field>
        <field name="endicia_size">REGULAR</field>

    </record>
